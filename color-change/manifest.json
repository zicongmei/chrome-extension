{
    "manifest_version": 3,
    "name": "Sample Background Changer",
    "version": "1.0",
    "description": "Changes the background color of visited pages to light coral.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}